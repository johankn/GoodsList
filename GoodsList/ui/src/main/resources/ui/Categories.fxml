<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="categoriesPane" disable="false" prefHeight="700.0" prefWidth="800.0" visible="true" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label alignment="CENTER" layoutX="250.0" layoutY="45.0" prefHeight="40.0" prefWidth="350.0" text="What are you selling?">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <Button fx:id="electronicsButton" alignment="CENTER" layoutX="250.0" layoutY="100.0" mnemonicParsing="false" onAction="#handleCategory" prefHeight="80.0" prefWidth="350.0" text="Electronics">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
      <Button fx:id="clothingButton" layoutX="250.0" layoutY="200.0" mnemonicParsing="false" onAction="#handleCategory" prefHeight="80.0" prefWidth="350.0" text="Clothing">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
      <Button fx:id="vehiclesButton" layoutX="250.0" layoutY="400.0" mnemonicParsing="false" onAction="#handleCategory" prefHeight="80.0" prefWidth="350.0" text="Vehicles">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
      <Button fx:id="booksButton" layoutX="250.0" layoutY="500.0" mnemonicParsing="false" onAction="#handleCategory" prefHeight="80.0" prefWidth="350.0" text="Books">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
      <Button fx:id="propertyButton" layoutX="250.0" layoutY="300.0" mnemonicParsing="false" onAction="#handleCategory" prefHeight="80.0" prefWidth="350.0" text="Property">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
      <Button fx:id="exitButton" layoutX="410.0" layoutY="625.0" mnemonicParsing="false" onAction="#handleExitButton" scaleX="2.0" scaleY="2.0" text="Exit" />
   </children>
</AnchorPane>
